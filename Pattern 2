#include <bits/stdc++.h>
using namespace std;
void print2(int n){
    for(int i=0;i<=n;i++){
        for(int j=1;j<=i;j++){
            cout<<j;
        }
        cout<<endl;
    }
}
int main(){
    int n,t;
    cout<<"enter an integer for test case";
    cin>>t;
    for(int i=0;i<t;i++){
        cout<<"enter integer for pattern";
        cin>>n;
        print2(n);
    }
    return 0;
}
