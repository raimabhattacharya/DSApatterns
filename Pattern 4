#include<bits/stdc++.h>
using namespace std;
void printp(int n){
    for(int i=0;i<=n;i++){
        for(int j=1;j<=i;j++){
            cout<<i;
        }
        cout<<endl;
    }
}
int main(){
    int t,n;
    cout<<"enter a number for test cases: ";
    cin>>t;
    for(int i=0;i<t;i++){
        cout<<"enter an integer for pattern: ";
        cin>>n;
        printp(n);
    }
    return 0;
}
