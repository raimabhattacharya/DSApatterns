#include<bits/stdc++.h>
using namespace std;
void printp(int n){
    for(int i=n;i>=0;i--){
        for(int j=i;j>0;j--){
            cout<<"* ";
        }
        cout<<endl;
    }
}
int main(){
    int n,t;
    cout<< "enter an integer for test cases:";
    cin>>t;
    for(int i=0;i<t;i++){
        cout<<"enter integer for pattern";
        cin>>n;
        printp(n);
    }
    return 0;
}
